{{#extend "stylesheets"}}
    <link rel="stylesheet" href="/stylesheets/admins/idVerifications.css" />
{{/extend}}

<ul>
    {{#each verifications}}
		<li>
            <ul>
                <li> User: {{this._user.firstName}}  {{this._user.lastName}} </li>
                <li> College: {{this.college}} </li>
                <li > Id Image: <img class="verification-image" src='{{this.idImagePath}}'/> </li>
                <li> Self Image: <img class="verification-image" src='{{this.selfImagePath}}'/>  </li>
                <li> <input type="button" name="Verify" /> <input type="button" name="Reject" /> </li>
            </ul>
        </li>
	{{/each}}
</ul>
